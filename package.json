{
  "name": "website",
  "private": true,
  "scripts": {
    "client:build": "npm run build --prefix client",
    "client:dev": "npm run dev --prefix client",
    "client:start": "npm run start --prefix client",
    "server:dev": "npm run dev --prefix server",
    "server:start": "npm run start --prefix server",
    "build": "npm run client:build",
    "start": "npm run server:start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "example": "concurrently \"npm run example --prefix server\" \"npm run client:dev\""
  },
  "workspaces": [
    "client",
    "server"
  ],
  "devDependencies": {
    "concurrently": "8.2.2",
    "cross-env": "10.1.0"
  }
}
