{
  "name": "website",
  "private": true,
  "scripts": {
    "client:build": "npm run build --prefix client",
    "client:dev": "npm run dev --prefix client",
    "client:start": "npm run start --prefix client",
    "server:dev": "npm run dev --prefix server",
    "server:start": "npm run start --prefix server",
    "build": "npm run client:build",
    "start": "npm run server:start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "example": "concurrently \"npm run example --prefix server\" \"npm run client:dev\""
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "react-dom": "^19.1.1",
    "uuid": "^11.1.0"
  }
}
